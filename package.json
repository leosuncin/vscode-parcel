{
	"name": "vscode-parcel",
	"displayName": "VSCode Parcel Bundler",
	"description": "Create project and run it with zero config",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [{
			"command": "extension.helloWorld",
			"title": "Hello World"
		}]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^13.1.8",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.0",
		"typescript": "^3.7.5",
		"tslint": "^5.12.1",
		"vscode-test": "^1.3.0"
	}
}
